{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "n8n",
  "title": "n8n",
  "description": "n8n Workflow launcher",
  "icon": "n8n.png",
  "author": "DesignLogicSolutions",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "required": true,
      "title": "n8n API Key",
      "description": "Your n8n API key for authentication",
      "placeholder": "Enter your n8n API key"
    },
    {
      "name": "baseUrl",
      "type": "textfield",
      "required": true,
      "title": "n8n Base URL",
      "description": "Your n8n instance URL (e.g., https://n8n.example.com)",
      "placeholder": "https://n8n.example.com"
    }
  ],
  "commands": [
    {
      "name": "search-workflows",
      "title": "Search Workflows",
      "description": "Search Workflows",
      "mode": "view"
    },
    {
      "name": "create-workflow",
      "title": "Create Workflow",
      "description": "Create a new n8n workflow",
      "mode": "no-view",
      "arguments": [
        {
          "name": "name",
          "placeholder": "Workflow Name",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "create-webhook",
      "title": "Create Webhook",
      "description": "Convert curl command to n8n webhook JSON",
      "mode": "view",
      "keywords": [
        "curl",
        "webhook",
        "n8n"
      ]
    },
    {
      "name": "parse-webhook",
      "title": "Parse Webhook",
      "description": "Convert n8n webhook JSON to curl command",
      "mode": "view",
      "keywords": [
        "curl",
        "webhook",
        "n8n",
        "json"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.91.2",
    "@raycast/utils": "^1.17.0",
    "node-fetch": "^3.3.2",
    "parse-curl": "^0.2.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "@types/uuid": "^9.0.8",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
